#TODO: Check if the system is a MAC or nix
# source the global file
if [ -f /etc/bashrc ]; then
        . /etc/bashrc   # --> Read /etc/bashrc, if present.
fi

# Add bash aliases.
if [ -f ~/.bash_aliases ]; then
    source ~/.bash_aliases
fi

#Add the development folder to path
export PATH=/Users/dsubramanian/development:$PATH

export P4PORT="p4fc.mygazoo.com:1999""