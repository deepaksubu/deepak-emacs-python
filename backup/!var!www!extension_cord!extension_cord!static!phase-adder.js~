function addPhase(){
    var phaseName = $("#phasename").val();
    //Call a url which maps to the add test phase function
    //Add the phaseName to the request.
    //If the request is successful, add the new option to the list of values
    //else do not do anything.
    var phaseURI = '/add_test_phase/' + phaseName
    
    $.get(phaseURI, function(data) {
	console.log(data)
	alert('Load was performed.');
    });

    $('#testPhaseSelector').append($("<option/>", {
        value: phaseName,
        text: phaseName
    }));
    
}