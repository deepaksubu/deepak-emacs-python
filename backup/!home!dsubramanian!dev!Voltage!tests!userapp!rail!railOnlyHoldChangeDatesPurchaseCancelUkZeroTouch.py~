"""
Test case -UK
"""
import voltage
from voltage.flow.login import rpa
from voltage.flow.travel.rearden import railUkOnly
from voltage.fragment.userapp import travel
class rpaRailOnly(railUkOnly.Flow):
    """
    Rail Only Purchase work flow

    """
    def __init__(self, test=None, browser=None, config=None, args=None):
        super(rpaRailOnly, self).__init__(test, browser, config, args)
        self.testcase_name='purchase_flow'
        self.tid = '1'
        self.country = 'UK'

    PARENT = railUkOnly.Flow
    
    

def test_main():
    test = voltage.Test()
    browser = test.browser()
    assert isinstance(browser, voltage.Browser) # Tip for Eclipse

    try:
        args = {
              "seed_type" : test.config["travel_regression"]["seed"]["generic_travel_seed_type"],
              "domain_type" : test.config["travel_regression"]["seed"][test.config["travel_regression"]["seed"]["generic_travel_seed_type"]]["generic_domain_type"],
              "user_type" : "user1_atoc"
               }
        args = travel.rpa_login(test, browser, rpa.Login, args)
        browser.execute(test, rpaRailOnly)
    except:
        test.handle_exception()

    return test.results()